{
  "compilerOptions": {
    "target": "ES2020",
    "module": "nodenext",            
    "moduleResolution": "nodenext",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "ESNext.BigInt"],
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,

  },
  "include": [
    "src",
    "worker.js",
    "src/testdb.ts"
  ]
}
